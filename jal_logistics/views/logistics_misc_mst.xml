<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!-- CHA -->
    <record id="cha_mst_form" model="ir.ui.view">
        <field name="name">cha.mst.form</field>
        <field name="model">cha.mst</field>
        <field name="arch" type="xml" >
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name" required="1" />
                        </group>
                    </group>                    
                </sheet>
                <div class="oe_chatter">
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>

    <record id="cha_mst_tree" model="ir.ui.view">
        <field name="name">cha.mst.tree</field>
        <field name="model">cha.mst</field>
        <field name="arch" type="xml" >
            <tree sample="1">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_cha_mst" model="ir.actions.act_window">
        <field name="name">CHA</field>
        <field name="res_model">cha.mst</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a New Record</p>
        </field>
    </record>

