<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="module_jal_logistics_cat" model="ir.module.category">
        <field name="name">Logistics</field>
    </record>

    <record id="res_group_logistics_user" model="res.groups">
        <field name="name">User</field>
        <field name="implied_ids" eval="False"/>
        <field name="category_id" ref="jal_logistics.module_jal_logistics_cat"/>
    </record>

    <record id="res_group_logistics_agent" model="res.groups">
        <field name="name">Agent</field>
        <field name="implied_ids" eval="[(4, ref('res_group_logistics_user'))]"/>
        <field name="category_id" ref="jal_logistics.module_jal_logistics_cat"/>
    </record>

    <record id="res_group_logistics_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="implied_ids" eval="[(4, ref('res_group_logistics_agent'))]"/>
        <field name="category_id" ref="jal_logistics.module_jal_logistics_cat"/>
    </record>

</odoo>
