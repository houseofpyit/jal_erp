<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sequence_jal_production" model="ir.sequence">
        <field name="name">production Req Seq</field>
        <field name="code">jal.production.seq</field>
        <field name="active">TRUE</field>
        <field name="prefix">PRO</field>
        <field name="company_id" eval="False"/>
        <field name="padding">5</field>
    </record>

    <record id="sequence_jal_quality" model="ir.sequence">
        <field name="name">quality Req Seq</field>
        <field name="code">jal.quality.seq</field>
        <field name="active">TRUE</field>
        <field name="prefix">QC</field>
        <field name="company_id" eval="False"/>
        <field name="padding">5</field>
    </record>

    <record id="sequence_jal_material_req" model="ir.sequence">
        <field name="name">Material Req Seq</field>
        <field name="code">jal.material.requisition.seq</field>
        <field name="active">TRUE</field>
        <field name="prefix">MR</field>
        <field name="company_id" eval="False"/>
        <field name="padding">5</field>
    </record>
    
</odoo>