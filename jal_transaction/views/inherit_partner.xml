<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="master_res_partner_form_view_inherit" model="ir.ui.view">
        <field name="name">master.res.partner.form.view.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="master.party_mst_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='remarks']" position="after">
                <field name="team_id" options="{'no_open': True, 'no_create': True}"/>
                <field name="business_type" />
            </xpath>
            <xpath expr="//field[@name='party_group_id']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='commission_rate']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='transport_id']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='agent_id']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='party_code']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//page[@name='rates']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='tcs_applicable']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//field[@name='tds_applicable']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//page[@name='rates']" position="before">
                <page name="account" string="Account" autofocus="autofocus">
                    <group>
                        <group>
                            <field name="tcs_applicable"/>
                            <field name="tds_applicable"/>
                        </group>   
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
